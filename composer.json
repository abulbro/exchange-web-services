{
  "name": "abulbro/lib-ews",
  "description": "Library for communicating with Exchange Web Services.",
  "homepage": "http://github.com/abulbro/exchange-web-services",
  "license": "MIT",
  "authors": [
    {
      "name": "abulbro",
      "email": "afazulul@my.bridgeport.edu"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": ">=5.3.9",
    "ext-soap": "*",
    "jamesiarmes/php-ntlm": "~1.0"
  },
  "require-dev": {
    "mustache/mustache": "~2.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "jamesiarmes\\PhpEws\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}